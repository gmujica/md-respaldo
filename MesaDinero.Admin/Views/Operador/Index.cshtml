                                <div class="fancy-input-content">
                                    <div class="fake-placeholder">Fecha de Nacimiento</div>
                                    <input data-bind="value:fechaNacimiento" type="text" disabled>
                                </div>
                            </fancy-input>
                            <fancy-input id="person-reg-email" class="selected" placeholder="E-mail">
