                                        <div class="fake-placeholder">Es una persona políticamente expuesta</div>
                                        <input data-bind="value:expuesto" type="text" disabled>
                                    </div>
                                </fancy-input>
                                @*<fancy-input class="selected" placeholder="Entidad Pública">
                            <div class="fancy-input-content">
                                <div class="fake-placeholder">Entidad Pública</div>
                                <input value="Ninguna" type="text" disabled>
                            </div>
                        </fancy-input>
                        <fancy-input class="selected" placeholder="Cargo">
                            <div class="fancy-input-content">
                                <div class="fake-placeholder">Cargo</div>
                                <input value="Ninguno" type="text" disabled>
                            </div>
                        </fancy-input>*@
                            </div>
                        </div>
                        <div class=block-1-4>
                            <div class="sub-title-normal">Cuentas Bancarias</div>
                            <bank-list>
                                <div class="bank-list">
                                   
                                    <table class="k-table k-thover k-trow-pointer">
                                        <thead class="k-thead">
                                            <tr class="k-throw">
                                                <th>Cuenta</th>
                                                <th>Banco</th>
                                                <th>Moneda</th>
                                                <th>Tipo de Cuenta</th>
                                                <th>Nro de Cuenta</th>
                                                <th>Estado</th>              
                                            </tr>
                                        </thead>
                                        <tbody class="k-tbody" data-bind="foreach:$data.cuentasBancarias">
                                            <tr>
                                                <td>
                                                    <span data-bind="text:'N°'  + ($index() + 1)"></span>
                                                </td>
                                                <td>
                                                    <span data-bind=""></span>
                                                    <img data-bind="attr:{'src': $data.logo}" width="100" />
                                                </td>
                                                <td>
                                                    <span data-bind="text:$data.moneda"></span>
                                                </td>
                                                <td>
                                                    <span data-bind="text:$data.tipoCuentaText"></span>
                                                </td>
                                                <td>
                                                    <span data-bind="text:$data.nroCuenta"></span>
                                                </td>
                                                <td>
                                                    <span data-bind="text:$data.estado"></span>
                                                </td>

                                            </tr>
                                        </tbody>
                                    </table>

                                </div>
                            </bank-list>
                        </div>


                        <div class=block-1-4>
                            <div class="sub-title-normal">Observaciones</div>
                            <bank-list>
                                <div class="bank-list">

                                    <table class="k-table k-thover k-trow-pointer">
                                        <thead class="k-thead">
                                            <tr class="k-throw">
                                                <th>Nroº</th>
                                                <th>Fecha Obs.</th>
                                                <th>Mensaje</th>
                                                <th>Observador</th>
                                                <th>Nombre Obs.</th>
                                                <th>Estado</th>
                                            </tr>
                                        </thead>
                                        <tbody class="k-tbody" data-bind="foreach:$data.observaciones">
                                            <tr>
                                                <td>
                                                    <span data-bind="text:'N°'  + ($index() + 1)"></span>
                                                </td>
                                                <td>
                                                    <span data-bind="text:$data.fechaOperador"></span>
                                                </td>
                                                <td>
                                                    <span data-bind="text:$data.mensaje"></span>
                                                </td>
                                                <td>
                                                    <span data-bind="text:$data.observador"></span>
                                                </td>
                                                <td>
                                                    <span data-bind="text:$data.nombre"></span>
                                                </td>
                                                <td>
                                                    <span data-bind="text:$data.nombreEstado"></span>
                                                </td>
                                              
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </bank-list>
                        </div>

                       

                        <div class="block-1-3">


                            <div class="observations">
                                <div class="observations-title">Comentario</div>
                                <div class="observations-textarea">
                                    <textarea data-bind="value:$root.comentario" rows="4" cols="50" spellcheck="true" style="z-index: auto; position: relative; line-height: normal; font-size: 13.3333px; transition: none 0s ease 0s; background: none 0% 0% / auto repeat scroll padding-box border-box rgb(255, 255, 255);"></textarea>
                                </div>
                            </div>

                            <div class="button-row">
                                <!-- ko if:estadoValidacion() == 'P' || estadoValidacionFideicomiso() == 'O' || estadoValidacionFideicomiso() == 'R' -->
                                <div class="btn btn-green" data-bind="click:$root.aprobarPCliente"><span class="btn-text">Aprobar</span></div>
                                <div class="btn btn-orange" data-bind="click:$root.observarPCliente"><span class="btn-text">Observado</span></div>
                                <div class="btn btn-red" data-bind="click:$root.rechazarPCliente"><span class="btn-text">Rechazado</span></div>
                                @* <div class="btn blue-bordered"><span class="btn-text">Adjuntar Archivo</span></div>*@
                                <!-- /ko -->
                            </div>
                        </div>
                    </div>
                </div>
            </person-registration>
        </div>
    </div>
</div>

