
<select class="custom-select" data-bind="options:$root.lstTipoDocumentoaccionista,optionsText:'text',optionsValue:'value',optionsCaption:'Tipo de Documento',value:tipoDocumento"></select>
<fancy-input placeholder="N° de Documento">
    <div class="fancy-input-content">
        <div class="fake-placeholder">N° de Documento</div>
        <input data-bind="value:$data.nroDocumento" maxlength="20" type="text">
    </div>
</fancy-input>
<fancy-input placeholder="Nombres">
    <div class="fancy-input-content">
        <div class="fake-placeholder">Nombres</div>
        <input data-bind="value:$data.nombres" maxlength="200" type="text" disabled>
    </div>
</fancy-input>
<fancy-input placeholder="Apellido Paterno">
